<article class="post">
    <div class="posts">
{{#each posts as |post| }}
<article class="card">
    <div class="card-header flex-row justify-space-between">
      <h2>
        <a href="/post/{{id}}">{{post.title}}</a>
      </h2>
      <div>
        Posted by {{post.user.username}} on {{format_date post.createdAt}}
      </div>
    </div>
  </article>
{{/each}}
</div>
</article>
